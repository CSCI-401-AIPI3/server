{
  // "extends": "@tsconfig/node16/tsconfig.json",
  "compilerOptions": {
    "typeRoots": [
      "node_modules/@types"
    ],
    "suppressImplicitAnyIndexErrors": true,
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es2021",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "sourceMap": true,
    "outDir": "dist",
    "baseUrl": ".",
    "noImplicitAny": true,
    "paths": {
      "*": [
        "node_modules/*"
      ]
    }
  },
  "include": [
    "src/**/*"
  ]
}